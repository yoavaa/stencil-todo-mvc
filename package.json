{
  "name": "stencil-comp",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-comp/stencil-comp.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs && cp src/index.css www/index.css",
    "github-site": "npm run build && git checkout gh-pages && rm -R build && cp -R www/* . && git add build && git add index.html && git add index.css && git add learn.json && git add todomvc-common-base.js && git add todomvc-common-base.css && git commit -m\"site update\" && git push && git checkout master",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^1.12.2",
    "@types/jest": "24.9.1",
    "@types/puppeteer": "2.0.1",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "puppeteer": "2.1.1"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "todomvc-common": "^1.0.5"
  }
}
